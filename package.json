{
  "name": "aes-syntaxhlr",
  "displayName": "May OS / AE-DOS Script Syntax Highlighting Support",
  "description": "this extension allows to highlight May OS's AE Script.",
  "version": "0.3.1",
  "license": "CC BY-NC 4.0", 
  "publisher": "NoteSwiper",
  "engines": {
    "vscode": "^1.75.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NoteSwiper/aes-syntaxhlr.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "images/icon/file-mayos_dark.png",
  "keywords": [
    "may",
    "mayos",
    "May",
    "MayOS",
    "May OS",
    "may os",
    "mayOS",
    "may OS",
    "AES",
    "aes",
    "aess",
    "aeasm",
    "aeini"
  ],
  "preview": true,
  "pricing": "Free",
  "activationEvents": [
    "onLanguage:mayosscript",
    "workspaceContains:*/.may/conf.xml",
    "workspaceContains:*/.may/path.may-code",
    "onCommand:_mayos.metadataFilesGeneration"
  ],
  "main": "./dist/extension",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "homepage": "https://github.com/NoteSwiper/aes-syntaxhlr/blob/main/README_home.md",
  "bugs": {
    "url": "https://github.com/NoteSwiper/aes-syntaxhlr/issues"
  },
  "contributes": {
    "customEditors": [
      {
        "viewType": "editing.mayos-script",
        "displayName": "May OS Script",
        "selector": [
          {
            "filenamePattern": "*.aes"
          }
        ]
      }
    ],
    "configurationDefaults": {
      "[aes]": {
        "editor.semanticHighlighting.enabled": true,
        "editor.semanticTokenColorCustomizations": {
          "[Default Dark+ Experimental]": {
            "enabled": true,
            "rules": {
              "keyword": "#8f8f00",
              "regexp": "#555fff",
              "number": "#f8f800",
              "namespace": "#db1a1a",
              "type": "#6ed445",
              "struct": "#7a80d6",
              "class": "#1b5f18",
              "enum": "#ff0000",
              "interface": "#8e8fdd",
              "typeParameter": "#bb8ba8",
              "parameter": "#366d64",
              "variable": "#9ea034",
              "property": "#759290",
              "enumMember": "#77678a",
              "decorator": "#5f8676",
              "event": "#ff5f89",
              "function": "#5fff53",
              "method": "#40f3f3",
              "macro": "#3f3f0f",
              "label": "#ff44f3",
              "comment": "#007f00",
              "string": "#7f8f7f",
              "operator": "#f5f500",
              "class.readonly": {
                "foreground": "#3b6e39",
                "fontStyle": "bold italic"
              },
              "*.definition": {
                "fontStyle": "italic bold",
                "foreground": "#b3b35c65"
              },
              "*.static": {
                "foreground": "#0000ff65"
              },
              "*.deprecated": {
                "foreground": "#ff000065",
                "fontStyle": "bold strikethrough"
              },
              "*.async": {
                "foreground": "#0000ff70",
                "fontStyle": "underline"
              },
              "*.modification": {
                "foreground": "#fff00070",
                "bold": true
              },
              "*.documentation": "#ff000090",
              "*.declaration": {
                "fontStyle": "bold"
              },
              "*.readonly": "#696869"
            }
          }
        }
      }
    },
    "configuration": {
      "type": "object",
      "title": "May OS / AE-DOS",
      "properties": {
        "aes-syntaxhlr.customargs": {
          "type": [
            "array",
            "null"
          ],
          "default": "null",
          "description": "Set Arguments on Execute.",
          "scope": "machine-overridable"
        },
        "aes-syntaxhlr.trace.server": {
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "If enabled, LSP would traces.",
          "scope": "window"
        }
      }
    },
    "languages": [
      {
        "id": "aes",
        "aliases": [
          "aescript",
          "A Easy Script (old name)",
          "aes",
          "AES"
        ],
        "extensions": [
          ".aes",
          ".ae",
          ".may-script",
          ".may",
          ".aesrg",
          ".aemay",
          ".maycode",
          ".maycd"
        ],
        "icon": {
          "light": "./images/icon/file-mayos_light.png",
          "dark": "./images/icon/file-mayos_dark.png"
        },
        "firstLine": "",
        "configuration": "./aes.language-configuration.json"
      },
      {
          "id": "aess",
          "extensions": [
              ".aess",
              ".may-script2",
              ".may2",
              ".aec"
          ],
          "aliases": [
              "AESS",
              "aess"
          ],
          "icon": {
            "light": "./images/icon/file-mayos_light.png",
            "dark": "./images/icon/file-mayos_dark.png"
          },
          "firstLine": "",
          "configuration": "./aess.language-configuration.json"
      },
      {
          "id": "aeasm",
          "extensions": [
              ".aeasm",
              ".mea",
              ".may-assembly",
              ".may-asm",
              ".mayasm",
              ".aecmp",
              ".maycmp",
              ".aecpl"
          ],
          "aliases": [
              "AEASM",
              "aeasm"
          ],
          "icon": {
            "light": "./images/icon/file-mayos_light.png",
            "dark": "./images/icon/file-mayos_dark.png"
          },
          "firstLine": "",
          "configuration": "./aeasm.language-configuration.json"
      },
      {
          "id": "aeini",
          "extensions": [
              ".aeini",
              ".aeconf",
              ".aesys",
              ".may-config",
              ".maycfg",
              ".aecfg"
          ],
          "aliases": [
              "AECFG",
              "aecfg"
          ],
          "icon": {
            "light": "./images/icon/file-mayos_light.png",
            "dark": "./images/icon/file-mayos_dark.png"
          },
          "firstLine": "",
          "configuration": "./aeini.language-configuration.json"
      }
    ],
    "resourceLabelFormatters": [
      {
        "scheme": "mayhub",
        "formatting": {
          "label": "${path}",
          "separator": "/",
          "workspaceSuffix": "May Hub"
        }
      }
    ],
    "breakpoints": [
      {
        "language": "aes"
      },
      {
        "language": "aess"
      },
      {
        "language": "aeasm"
      }
    ],
    "grammars": [
      {
        "language": "aes",
        "scopeName": "source.aes",
        "path": "./syntaxes/aes.tmLanguage.json"
      },
      {
          "language": "aess",
          "scopeName": "script.aes.aess",
          "path": "./syntaxes/aess.tmLanguage.json"
      },
      {
          "language": "aeasm",
          "scopeName": "script.aes.aeasm",
          "path": "./syntaxes/aeasm.tmLanguage.json"
      },
      {
          "language": "aeini",
          "scopeName": "script.aes.aeini",
          "path": "./syntaxes/aeini.tmLanguage.json"
      }
    ],
    "semanticTokenTypes": [
      { "id": "annotation", "superType": "type", "description": "" },
      { "id": "annotationMember", "superType": "method", "description": "" },
      { "id": "modifier", "superType": "keyword", "description": "A template script type." },
      { "id": "modifier", "superType": "keyword", "description": "A template script type." }
    ],
    "semanticTokenModifiers": [
      { "id": "public", "description": "the thing are here" },
      { "id": "private", "description": "the thing are here" },
      { "id": "protected", "description": "the thing are here" },
      { "id": "native", "description": "the thing are here" },
      { "id": "generic", "description": "the thing are here" },
      { "id": "typeArgument", "description": "the thing are here" },
      { "id": "importDeclaration", "description": "the thing are here" },
      { "id": "constructor", "description": "the thing are here" }
    ],
    "semanticTokenScopes": [
      { "language": "aes", "scopes": { "annotation": [ "storage.type.annotation.aes" ], "annotationMember": [ "entity.name.annotationMember.aes" ], "modifier": [ "storage.modifier.aes" ], "keyword.documentation": [ "keyword.other.documentation.aedoc.aes" ], "*.constructor": [ "entity.name.function.aes" ] } }
    ],
    "snippets": [
      {
        "language": "aes",
        "path": "./snippets/aes.snippet.json"
      },
      {
        "language": "aess",
        "path": "./snippets/aess.snippet.json"
      },
      {
        "language": "aeasm",
        "path": "./snippets/aeasm.snippet.json"
      },
      {
        "language": "aeini",
        "path": "./snippets/aeini.snippet.json"
      }
    ],
    "commands": [
      {
        "command": "aes-syntaxhlr.init.project",
        "title": "%aes-syntaxhlr.init.project%",
        "category": "MayOS"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "deploy": "vsce publish --yarn"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/glob": "5.0.30",
    "@types/lodash.findindex": "^4.6.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/vscode-webview": "^1.57.0",
    "@types/semver": "^7.3.8",
    "@types/sinon": "^10.0.12",
    "@types/vscode": "^1.77.0",
    "@types/winreg": "^1.2.30",
    "@types/winston": "^2.4.4",
    "@vscode/test-electron": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.13.0",
    "eslint-webpack-plugin": "^3.2.0",
    "gulp": "^4.0.2",
    "gulp-decompress": "^3.0.0",
    "gulp-download": "0.0.1",
    "lodash.findindex": "^4.6.0",
    "lodash.template": ">=4.5.0",
    "minimist": ">=1.2.6",
    "mocha": "^9.2.2",
    "request": "^2.88.2",
    "sinon": "^14.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.6.4",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.6.0",
    "@vscode/vsce": "^2.19.0"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.32",
    "@vscode/webview-ui-toolkit": "1.2.2",
    "chokidar": "^3.5.3",
    "@redhat-developer/vscode-redhat-telemetry": "^0.6.1",
    "@redhat-developer/vscode-extension-proposals": "0.0.21",
    "expand-home-dir": "^0.0.3",
    "fmtr": "^1.1.2",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.3",
    "htmlparser2": "6.0.1",
    "jdk-utils": "^0.4.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semver": "^7.3.5",
    "vscode-languageclient": "8.1.0",
    "winreg-utf8": "^0.1.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  }
}
